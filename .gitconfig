[core]
	excludesfile = ~/.gitignore
	fsyncobjectfiles = true
	compression = 9
[user]
	name = Josh Lee
	email = jleedev@gmail.com
	signingkey = "AFF87338"
[alias]
	stat = status
	ci = commit
	co = checkout
	w = whatchanged
	tnylog = log --oneline --graph --decorate
	dc = diff --cached
	tags = log -n1 --pretty=format:%h%d
[push]
	default = matching
[color]
	ui = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[github]
	user = jleedev
[web]
	browser = open
